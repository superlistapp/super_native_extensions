{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "clipboard example",
            "cwd": "super_clipboard/example",
            "request": "launch",
            "type": "dart",
            "env": {
                "DISPLAY": ":0"
            },
        },
        {
            "name": "clipboard example (profile mode)",
            "cwd": "super_clipboard/example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "env": {
                "DISPLAY": ":0"
            }
        },
        {
            "name": "clipboard example (release mode)",
            "cwd": "super_clipboard/example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "env": {
                "DISPLAY": ":0"
            }
        },
        {
            "name": "drag and drop example",
            "cwd": "super_drag_and_drop/example",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
            //  "--local-engine=host_debug_unopt"
            ],
            "env": {
                "DISPLAY": ":0"
            }
        },
        {
            "name": "drag and drop example (profile mode)",
            "cwd": "super_drag_and_drop/example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "env": {
                "DISPLAY": ":0"
            }
        },
        {
            "name": "drag and drop example (release mode)",
            "cwd": "super_drag_and_drop/example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "env": {
                "DISPLAY": ":0"
            }
        },
        {
            "name": "(Windows) Attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "symbolOptions": {
                "searchPaths": [],
                "searchMicrosoftSymbolServer": false
            },
        }
    ]
}